{
    "multiCommand.commands": [
        {
            "command": "extension.multiCommand.openFileAndCloseSidebar",
            "sequence": [
                "list.select",
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "command": "extension.multiCommand.openSearch",
            "sequence": [
                "workbench.action.findInFiles",
                "search.action.clearSearchResults"
            ]
        },
        {
            "command": "extension.multiCommand.openNextAndBackSidebar",
            "sequence": [
                "list.focusDown",
                "list.select",
                "workbench.action.focusSideBar"
            ],
            "interval": 400
        },
        {
            "command": "extension.multiCommand.openPrevAndBackSidebar",
            "sequence": [
                "list.focusUp",
                "list.select",
                "workbench.action.focusSideBar"
            ],
            "interval": 400
        },
        {
            "command": "extension.multiCommand.openChanges",
            "sequence": [
                "workbench.scm.focus",
                "list.collapseAll",
                "list.focusLast",
                "list.expand"
            ]
        },
        {
            "command": "extension.multiCommand.stageAndBackChanges",
            "sequence": [
                "git.stage",
                "extension.multiCommand.openChanges"
            ]
        },
    ],
    "workbench.colorTheme": "GitHub Dark High Contrast",
    "workbench.iconTheme": "vscode-icons",
}